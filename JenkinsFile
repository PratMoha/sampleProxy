pipeline {
  agent any
  stages {
    
    stage('Build APIProxy Bundle') {
    
      steps {
          // send build started notifications
       
       

      zip dir: 'testing', glob: '', zipFile: 'CI_CD_PROXY.zip'
      bat 'sh user=tester'
      }
    }

    stage('Deploy APIProxy Bundle') {
    
      steps {
          // send build started notifications
       
        bat 'sh ./deploytest.sh'
      }
    }

    
  }
 
}